{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "transformationadf"
		},
		"AzureEmptyDataBase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureEmptyDataBase'"
		},
		"AzureSqlSampleDBLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlSampleDBLS'"
		},
		"SelfHostedSQLServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SelfHostedSQLServer'"
		},
		"TrialDBLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'TrialDBLS'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "transformationadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureEmptyDataBase_connectionString": {
						"value": "[parameters('AzureEmptyDataBase_connectionString')]"
					},
					"AzureSqlSampleDBLS_connectionString": {
						"value": "[parameters('AzureSqlSampleDBLS_connectionString')]"
					},
					"TrialDBLS_connectionString": {
						"value": "[parameters('TrialDBLS_connectionString')]"
					},
					"SelfHostedSQLServer_connectionString": {
						"value": "[parameters('SelfHostedSQLServer_connectionString')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}